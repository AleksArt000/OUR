[info]
name = sqlite
version = 3400000
url = https://www.sqlite.org/2022/sqlite-autoconf-3400000.tar.gz

[download]
curl https://www.sqlite.org/2022/sqlite-autoconf-3400000.tar.gz | tar -xz
mv $NAME-autoconf-$VERSION $NAME-$VERSION

[install]
./configure --prefix=/usr
make 
make DESTDIR=$BUILD_ROOT install
