[info]
name = libxau
type = src
version = 1.0.11
url = https://www.x.org/pub/individual/lib/libXau-1.0.11.tar.xz]

[dependencies]
xorgproto

[description]
The libXau package contains a library implementing the X11 Authorization Protocol. This is useful for restricting client access to the display.

[install]
./configure $XORG_CONFIG &&
make
make install

