[info]
name = xcb-util-keysyms
type = src
version = 0.4.1
url = https://xcb.freedesktop.org/dist/xcb-util-keysyms-0.4.1.tar.xz

[dependencies]
libxcb

[description]
The xcb-util-keysyms package contains a library for handling standard X key constants and conversion to/from keycodes. 

[install]
wget $URL
tar -xf xcb-util-keysyms-0.4.1.tar.xz
cd xcb-util-keysyms-0.4.1
./configure $XORG_CONFIG &&
make
make install
