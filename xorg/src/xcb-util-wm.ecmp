[info]
name = xcb-util-wm
type = src
version = 0.4.2
url = https://xcb.freedesktop.org/dist/xcb-util-wm-0.4.2.tar.xz

[dependencies]
libxcb

[description]
The xcb-util-wm package contains libraries which provide client and window-manager helpers for EWMH and ICCCM. 

[install]
wget $URL
tar -xf xcb-util-wm-0.4.2.tar.xz
cd xcb-util-wm-0.4.2
./configure $XORG_CONFIG &&
make
make install
